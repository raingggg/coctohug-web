<div class="page-wrapper">
  <table class="table table-striped table-hover generalMessageTable">
    <thead>
      <tr>
        <th class="se_no">#</th>
        <th class="blockchain">
          <%= __('general.blockchain') %>
        </th>
        <th class="mode">
          <%= __('general.mode') %>
        </th>
        <th class="versions">
          <%= __('general.versions') %>
        </th>
        <th class="updatedAt">
          <%= __('general.updatedAt') %>
        </th>
        <th class="hostname">
          <%= __('general.hostname') %>
        </th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(function(dt, index){ %>
        <tr>
          <td class="se_no">
            <input type="checkbox" name="handcheck" value="<%= index %>" data-hostname="<%= dt.hostname %>"
              data-blockchain="<%= dt.blockchain %>" />
            <%= index + 1 %>
          </td>
          <td class="blockchain">
            <img src="images/logos/<%= dt.blockchain %>-min.png" alt="<%= dt.blockchain %>" width="20" height="20">
            <b>
              <%= dt.blockchain %>
            </b>
          </td>
          <td class="mode">
            <%= dt.mode %>
          </td>
          <td class="versions">
            <%- dt.versions && dt.versions.replace(/\n/g, "</br>" ) %>
          </td>
          <td class="updatedAt">
            <%= dt.updatedAt.toLocaleString("en-US", {hour12: false}) %>
          </td>
          <td class="blockchain">
            <%= dt.hostname %>
          </td>
        </tr>
        <% }); %>
    </tbody>
  </table>

  <div class="container">
    <div class="row">
      <div class="col-6">
        <button class="btn btn-outline-success" id="btnHandRemove">
          <%= __('hands.remove_selected') %>
        </button>
      </div>
    </div>
    <p></p>
  </div>
</div>