<div class="page-wrapper">
  &nbsp;<input class="search blockchain-fuzzy-search" type="search" data-column="1"
    placeholder='(blockchain; fuzzy search... try "flax")'>
  <table class="table table-striped table-hover generalMessageTable">
    <thead>
      <tr>
        <td>#</td>
        <td>
          <%= __('general.blockchain') %>
        </td>
        <td>
          <%= __('general.actions') %>
        </td>
        <td>
          <%= __('general.updatedAt') %>
        </td>
        <td>
          <%= __('general.hostname') %>
        </td>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(function(dt, index){ %>
        <tr>
          <td>
            <%= index + 1 %>
          </td>
          <td>
            <img src="/images/logos/<%= dt.blockchain %>-min.png" alt="<%= dt.blockchain %>" width="20" height="20">
            <b>
              <%= dt.blockchain %>
            </b>
          </td>
          <td><button class="btn btn-outline-success restart" data-hostname="<%= dt.hostname %>"
              data-blockchain="<%= dt.blockchain %>">
              <%= __('leftbar.restart') %>
            </button></td>
          <td>
            <%= dt.updatedAt.toLocaleString("en-US", {hour12: false}) %>
          </td>
          <td>
            <%= dt.hostname %>
          </td>
        </tr>
        <% }); %>
    </tbody>
  </table>
</div>